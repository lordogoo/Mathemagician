<table>
<tr>
<td>
<%- include('test',{title: 'add number fraction',menu: 'fraction_add_1',equation: '5=5/2+3/2',actionList: [{item: '4',action: 'preformAdd'}],result: '5=(5+3)/2'}); -%>
</td>
<td>
<%- include('test',{title: 'add number fraction',menu: 'fraction_add_2',equation: '5=(x+1)/3+(x+1)/3',actionList: [{item: '7',action: 'preformAdd'}],result: '5=((x+1)+(x+1))/3'}); -%>
